{
  "name": "@korfbal-stream-kit/korfbal-stream-apps",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many -t serve --all",
    "build": "nx run-many -t build --all",
    "test": "nx affected -t test",
    "api:dev": "npm run prisma:generate && dotenv -e apps/korfbal-stream-api/.env -- nx dev @korfbal-stream-kit/korfbal-stream-api --verbose",
    "api:build": "nx build @korfbal-stream-kit/korfbal-stream-api",
    "api:package": "nx package @korfbal-stream-kit/korfbal-stream-api",
    "api:serve": "npm run prisma:generate && dotenv -e apps/korfbal-stream-api/.env -- nx serve @korfbal-stream-kit/korfbal-stream-api --verbose",
    "api:test": "nx test @korfbal-stream-kit/korfbal-stream-api",
    "api:test:integration": "dotenv -e apps/korfbal-stream-api/.env -- vitest run apps/korfbal-stream-api/src/routes/sponsors.int.spec.ts --environment=node",
    "kit:dev": "nx serve @korfbal-stream-kit/korfbal-stream-kit",
    "kit:build": "nx build @korfbal-stream-kit/korfbal-stream-kit",
    "kit:test": "nx test @korfbal-stream-kit/korfbal-stream-kit",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "prisma:generate": "dotenv -e apps/korfbal-stream-api/.env -- prisma generate --schema=apps/korfbal-stream-api/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e apps/korfbal-stream-api/.env -- prisma migrate dev --schema=apps/korfbal-stream-api/prisma/schema.prisma",
    "prisma:deploy": "dotenv -e apps/korfbal-stream-api/.env -- prisma migrate deploy --schema=apps/korfbal-stream-api/prisma/schema.prisma",
    "prisma:seed": "dotenv -e apps/korfbal-stream-api/.env -- tsx apps/korfbal-stream-api/prisma/seed.ts",
    "prisma:studio": "dotenv -e apps/korfbal-stream-api/.env -- prisma studio --schema=apps/korfbal-stream-api/prisma/schema.prisma",
    "generate:sponsors:csv": "tsx ./scripts/generate-sponsor-csv.ts",
    "clean": "nx reset && rm -rf node_modules dist .nx/cache tmp",
    "reinstall": "npm run clean && npm i"
  },
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.17.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.6.4",
    "@nx/docker": "^21.6.4",
    "@nx/esbuild": "^21.6.4",
    "@nx/eslint": "21.6.4",
    "@nx/eslint-plugin": "21.6.4",
    "@nx/express": "^21.6.4",
    "@nx/jest": "21.6.4",
    "@nx/js": "^21.6.4",
    "@nx/node": "^21.6.4",
    "@nx/playwright": "21.6.4",
    "@nx/react": "21.6.4",
    "@nx/vite": "21.6.4",
    "@nx/web": "21.6.4",
    "@nx/webpack": "21.6.4",
    "@nx/workspace": "21.6.4",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "16.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "10.4.13",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.6.4",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "supertest": "^7.0.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  }
}
