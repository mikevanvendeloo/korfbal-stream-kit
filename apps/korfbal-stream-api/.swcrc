{
  "jsc": {
    "parser": { "syntax": "typescript", "tsx": false, "decorators": true, "dynamicImport": true },
    "transform": {
          "legacyDecorator": true,
          "decoratorMetadata": true
        },
    "target": "es2021",
    "externalHelpers": true,
    "loose": false
  },
  "module": { "type": "commonjs", "strict": false, "noInterop": false },
  "sourceMaps": true
}
